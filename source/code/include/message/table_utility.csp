<?php
require_once("fw/database_resource.csp");
require_once("grn/table_util.csp");
/**
 * @param integer $uid
 * @param string  $table_name
 *
 * @return string
 */
function grn_get_personal_tablename_message($uid, $table_name)
{
    return grn_get_personal_table_name_with_suffix($uid, $table_name);
}

/**
 * @param integer $uid
 *
 * @return string
 */
function grn_get_personal_suffix_message($uid)
{
    return grn_get_personal_suffix($uid);
}

