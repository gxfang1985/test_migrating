<?php

namespace grn\kunai\data\bean\log;

use grn\space\common\data\LogInterface;

class KunaiAvailabilityDeleteAllLog implements LogInterface
{
    const ACTION_DELETE = 'delete';
    const OBJECT_AVAILABILITY_DELETE_ALL = 'availability_user_delete_all';

    /**
     * @var string
     */
    private $_action;

    /**
     * @var string
     */
    private $_object;

    public function __construct()
    {
        $this->setAction(self::ACTION_DELETE);
        $this->setObject(self::OBJECT_AVAILABILITY_DELETE_ALL);
    }

    /**
     * @param string $action
     */
    public function setAction($action)
    {
        $this->_action = $action;
    }

    /**
     * @return string
     */
    public function getAction()
    {
        return $this->_action;
    }

    /**
     * @param string $object
     */
    public function setObject($object)
    {
        $this->_object = $object;
    }

    /**
     * @return string
     */
    public function getObject()
    {
        return $this->_object;
    }

    public function getParameters()
    {
        return [];
    }
}
