<?php

namespace grn\favour\data\collection;

use grn\grn\bean\BeanAbstract;
use grn\grn\collection\BeanCollectionAbstract;

class FavourNotifyCollection extends BeanCollectionAbstract
{
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * @access    public
     *
     * @param     BeanAbstract $bean
     */
    public function appendBean(BeanAbstract $bean)
    {
        assert('$bean instanceof grn\\favour\\data\\bean\\FavourNotify');
        parent::appendBean($bean);
    }
}
