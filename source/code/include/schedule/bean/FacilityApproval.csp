<?php

namespace grn\schedule\bean;

class FacilityApproval
{
    private $_colId;
    private $_colEvent;
    private $_colFacility;
    private $_colFacilityName;
    private $_colStatus;
    private $_colApprovalTime;
    private $_colComment;
    private $_colUser;
    private $_colUserName;
    private $_colUsing;
    private $_colStartDateTime;
    private $_colEndDateTime;

    const STATUS_REJECT = 0;
    const STATUS_APPROVE = 1;
    const NOT_USING = 0;
    const USING = 1;

    public function __construct($inDataArray = null)
    {
        if (is_null($inDataArray)) {
            $this->setId('');
            $this->setEvent('');
            $this->setFacility('');
            $this->setFacilityName('');
            $this->setStatus('');
            $this->setApprovalTime('');
            $this->setComment('');
            $this->setUser('');
            $this->setUserName('');
            $this->setUsing('');
            $this->setStartDateTime('');
            $this->setEndDateTime('');
        } else {
            $this->setId(cb_at($inDataArray, '_id', ''));
            $this->setEvent(cb_at($inDataArray, 'col_event', ''));
            $this->setFacility(cb_at($inDataArray, 'col_facility', ''));
            $this->setFacilityName(cb_at($inDataArray, 'col_facility_name',
                ''));
            $this->setStatus(cb_at($inDataArray, 'col_status', ''));
            $this->setApprovalTime(cb_at($inDataArray, 'col_approval_time',
                ''));
            $this->setComment(cb_at($inDataArray, 'col_comment', ''));
            $this->setUser(cb_at($inDataArray, 'col_user', ''));
            $this->setUserName(cb_at($inDataArray, 'col_user_name', ''));
            $this->setUsing(cb_at($inDataArray, 'col_using', ''));
            $this->setStartDateTime(cb_at($inDataArray, 'col_start_datetime',
                ''));
            $this->setEndDateTime(cb_at($inDataArray, 'col_end_datetime', ''));
        }
    }

    public function toArray()
    {
        $result = [];
        $result['_id'] = $this->getId();
        $result['col_event'] = $this->getEvent();
        $result['col_facility'] = $this->getFacility();
        $result['col_facility_name'] = $this->getFacilityName();
        $result['col_status'] = $this->getStatus();
        $result['col_approval_time'] = $this->getApprovalTime();
        $result['col_comment'] = $this->getComment();
        $result['col_user'] = $this->getUser();
        $result['col_user_name'] = $this->getUserName();
        $result['col_using'] = $this->getUsing();
        $result['col_start_datetime'] = $this->getStartDateTime();
        $result['col_end_datetime'] = $this->getEndDateTime();

        return $result;
    }

    public function getId()
    {
        return $this->_colId;
    }

    public function setId($value)
    {
        $this->_colId = $value;
    }

    public function getEvent()
    {
        return $this->_colEvent;
    }

    public function getEventId()
    {
        if (is_object($this->_colEvent)) {
            return $this->_colEvent->id;
        }

        return $this->_colEvent;
    }

    public function setEvent($value)
    {
        $this->_colEvent = $value;
    }

    public function getFacility()
    {
        return $this->_colFacility;
    }

    public function setFacility($value)
    {
        $this->_colFacility = $value;
    }

    public function getFacilityName()
    {
        return $this->_colFacilityName;
    }

    public function setFacilityName($value)
    {
        $this->_colFacilityName = $value;
    }

    public function getStatus()
    {
        return $this->_colStatus;
    }

    public function setStatus($value)
    {
        $this->_colStatus = $value;
    }

    public function getApprovalTime()
    {
        return $this->_colApprovalTime;
    }

    public function setApprovalTime($value)
    {
        $this->_colApprovalTime = $value;
    }

    public function getComment()
    {
        return $this->_colComment;
    }

    public function setComment($value)
    {
        $this->_colComment = $value;
    }

    public function getUser()
    {
        return $this->_colUser;
    }

    public function setUser($value)
    {
        $this->_colUser = $value;
    }

    public function getUserName()
    {
        return $this->_colUserName;
    }

    public function setUserName($value)
    {
        $this->_colUserName = $value;
    }

    public function getUsing()
    {
        return $this->_colUsing;
    }

    public function setUsing($value)
    {
        $this->_colUsing = $value;
    }

    public function isApproved()
    {
        return $this->_colStatus == '1' ? true : false;
    }

    public function isRejected()
    {
        return $this->_colStatus == '0' ? true : false;
    }

    public function isUsing()
    {
        return $this->_colUsing == '1' ? true : false;
    }

    public function setStartDateTime($startDateTime)
    {
        $this->_colStartDateTime = $startDateTime;
    }

    public function setEndDateTime($endDateTime)
    {
        $this->_colEndDateTime = $endDateTime;
    }

    public function getStartDateTime()
    {
        return $this->_colStartDateTime;
    }

    public function getEndDateTime()
    {
        return $this->_colEndDateTime;
    }
}
