<?php

namespace grn\fts;

class DocumentView
{
    /** @var string $snippet */
    private $snippet;
    /** @var string $title */
    private $title;
    /** @var string $titleIcon */
    private $titleIcon;
    /** @var string $url */
    private $url;
    /** @var ModifierView $modifierView */
    private $modifierView = null;
    /** @var string $modifiedTime */
    private $modifiedTime;
    /** @var FileView $fileView */
    private $fileView;
    /** @var SpaceView $spaceView */
    private $spaceView;
    /** @var AddressView $addressView */
    private $addressView = null;

    /**
     * @param string $snippet
     * @param string $title
     * @param string $titleIcon
     * @param string $url
     * @param string $modifiedTime
     */
    public function __construct(
        $snippet,
        $title,
        $titleIcon,
        $url,
        $modifiedTime
    ) {
        $this->setSnippet($snippet);
        $this->setTitle($title);
        $this->setTitleIcon($titleIcon);
        $this->setUrl($url);
        $this->setModifiedTime($modifiedTime);
    }

    /**
     * @param string $modifiedTime
     */
    public function setModifiedTime($modifiedTime)
    {
        $this->modifiedTime = $modifiedTime;
    }

    /**
     * @return string
     */
    public function getModifiedTime()
    {
        return $this->modifiedTime;
    }

    /**
     * @param \grn\fts\ModifierView|string $modifierView
     */
    public function setModifierView($modifierView)
    {
        $this->modifierView = $modifierView;
    }

    /**
     * @return \grn\fts\ModifierView|string
     */
    public function getModifierView()
    {
        return $this->modifierView;
    }

    /**
     * @param string $snippet
     */
    public function setSnippet($snippet)
    {
        $this->snippet = $snippet;
    }

    /**
     * @return string
     */
    public function getSnippet()
    {
        return $this->snippet;
    }

    /**
     * @param string $title
     */
    public function setTitle($title)
    {
        $this->title = $title;
    }

    /**
     * @return string
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * @param string $url
     */
    public function setUrl($url)
    {
        $this->url = $url;
    }

    /**
     * @return string
     */
    public function getUrl()
    {
        return $this->url;
    }

    /**
     * @param string $iconUrl
     */
    public function setTitleIcon($iconUrl)
    {
        $this->titleIcon = $iconUrl;
    }

    /**
     * @return string
     */
    public function getTitleIcon()
    {
        return $this->titleIcon;
    }

    /**
     * @param \grn\fts\FileView $fileView
     */
    public function setFileView(FileView $fileView)
    {
        $this->fileView = $fileView;
    }

    /**
     * @return \grn\fts\FileView
     */
    public function getFileView()
    {
        return $this->fileView;
    }

    /**
     * @param \grn\fts\SpaceView $spaceView
     */
    public function setSpaceView(SpaceView $spaceView)
    {
        $this->spaceView = $spaceView;
    }

    /**
     * @return \grn\fts\SpaceView
     */
    public function getSpaceView()
    {
        return $this->spaceView;
    }

    /**
     * @param \grn\fts\AddressView $addressView
     */
    public function setAddressView(AddressView $addressView)
    {
        $this->addressView = $addressView;
    }

    /**
     * @return \grn\fts\AddressView
     */
    public function getAddressView()
    {
        return $this->addressView;
    }
}
