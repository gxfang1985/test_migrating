<?php

/**
 * Class WeatherLocationsResource
 *
 * This file is resource file of locations of weather forecast.
 * This file is Japanese only because Bee(cb web service) supports only Japanese.
 */

class WeatherLocationsResource
{
    /**
     * @param array $checked_locations Array of "{prefectureCode}-{pointCode}". ex. ["1-214", "23-106"]
     *
     * @return array
     */
    public static function getLocationTree($checked_locations = [])
    {
        $location_tree = self::getLocationTreeData();
        foreach ($location_tree as &$region_info) {
            $region_info['checked'] = false;

            foreach ($region_info['prefectures'] as &$prefecture_info) {
                foreach ($prefecture_info['points'] as &$point_info) {
                    $point_info['checked'] = false;
                    if (in_array($point_info['code'], $checked_locations)) {
                        $region_info['checked'] = true;
                        $point_info['checked'] = true;
                    }
                }
            }
        }

        return $location_tree;
    }

    /**
     * @return array
     */
    public static function getAllLocations()
    {
        $location_list = self::getLocationTreeData();
        $location_list_formatted = [];
        foreach ($location_list as $location) {
            $prefectures = $location['prefectures'];
            foreach ($prefectures as $prefecture) {
                $points = $prefecture['points'];
                foreach ($points as $point) {
                    $location_list_formatted[$point['code']] = $point['name'];
                }
            }
        }

        return $location_list_formatted;
    }

