<?php
declare(strict_types=1);

namespace grn\grn\push_notification\bean;

class Receiver
{
    /**
     * @var int
     */
    public $userId;

    /** @var \stdClass */
    public $placeHolder;

    /**
     * Receiver constructor.
     *
     * @param int       $user_id
     * @param \stdClass $place_holder
     */
    function __construct(
        int $user_id,
        \stdClass $place_holder = null
    ) {
        $this->userId = $user_id;
        if (is_null($place_holder)) {
            $this->placeHolder = new \stdClass();
        } else {
            $this->placeHolder = $place_holder;
        }
    }
}
