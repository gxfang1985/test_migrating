<?php
declare(strict_types=1);

namespace grn\grn\push_notification\bean;

class APNSContent
{
    /**
     * @var string
     */
    public $abstract_url;

    /**
     * @var Aps
     */
    public $aps;

    /**
     * @var int
     */
    public $version;

    /**
     * @param string $abstract_url
     * @param Aps    $aps
     * @param int    $version
     */
    function __construct(
        string $abstract_url,
        Aps $aps,
        int $version
    ) {
        $this->abstract_url = $abstract_url;
        $this->aps = $aps;
        $this->version = $version;
    }

}
