<?php
declare(strict_types=1);

namespace grn\grn\push_notification\bean;

class Aps
{
    /**
     * @var string
     */
    public $alert;

    /**
     * @var string
     */
    public $sound;

    /**
     * Aps constructor.
     *
     * @param string $alert
     * @param string $sound
     */
    function __construct(
        string $alert,
        string $sound
    ) {
        $this->alert = $alert;
        $this->sound = $sound;
    }
}
