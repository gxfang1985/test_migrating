<?php
declare(strict_types=1);

namespace grn\grn\push_notification\bean;

class Data
{
    /**
     * @var string
     */
    public $title;

    /**
     * @var string
     */
    public $body;

    /**
     * @var string
     */
    public $abstract_url;

    /**
     * @var string
     */
    public $domain;

    /**
     * @var string
     */
    public $service;

    /**
     * @var string
     */
    public $user;

    /**
     * @var int
     */
    public $version;

    /**
     * Data constructor.
     *
     * @param string $title
     * @param string $body
     * @param string $abstract_url
     * @param string $domain
     * @param string $service
     * @param string $user_code
     * @param int    $version
     */
    function __construct(
        string $title,
        string $body,
        string $abstract_url,
        string $domain,
        string $service,
        string $user_code,
        int $version
    ) {
        $this->title = $title;
        $this->body = $body;
        $this->abstract_url = $abstract_url;
        $this->domain = $domain;
        $this->service = $service;
        $this->user = $user_code;
        $this->version = $version;
    }

}
