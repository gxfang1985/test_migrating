<?php

namespace grn\grn\customization\common;

class CustomizationSetting
{
    private function __construct()
    {
    }

    /**
     * @return bool
     */
    public static function isForceDisabled()
    {
        $profile = cb_get_system_profile('grn.common.js_customization');

        $is_disabled = false;
        $profile->getAttribute('force_disabled', $is_disabled);

        return $is_disabled;
    }
}
