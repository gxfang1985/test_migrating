<?php

namespace grn\grn\access\dao;


use grn\grn\access\bean\Availability;

class AvailabilityDynamicRoleDAO extends AvailabilityBaseDAO
{

    /**
     * AvailabilityDynamicRoleDAO constructor.
     */
    function __construct()
    {
        parent::__construct();
        $this->table = "tab_grn_availability_dynamic_role";
        $this->targetColumn = "col_dynamic_role";
    }

    /**
     * @param array $raw_data
     *
     * @return Availability
     */
    protected function convertArrayToBean(array $raw_data)
    {
        $availability = parent::convertArrayToBean($raw_data);
        $availability->setTargetType(Availability::DYNAMIC_ROLE);

        return $availability;
    }

}
