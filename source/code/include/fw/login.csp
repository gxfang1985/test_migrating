<?php

/**
 * Get parameters without login parameters.
 *
 * @return array
 */
function cb_get_page_params()
{
    global $G_INPUT;
    $params = $G_INPUT;
    unset($params['_system']);
    unset($params['_uid']);
    unset($params['_account']);
    unset($params['_password']);

    return $params;
}
