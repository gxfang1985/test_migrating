    public function convertArray()
    {
        $result = [];

        $result['_id'] = $this->getId();
        $result['col_active'] = $this->getActive();
        $result['col_creator'] = $this->getCreator();
        $result['col_creator_foreign_key'] = $this->getCreatorForeignKey();
        $result['col_creator_name'] = $this->getCreatorName();
        $result['col_ctime'] = $this->getCtime();
        $result['col_list_index'] = $this->getListIndex();
        $result['col_modifier'] = $this->getModifier();
        $result['col_modifier_foreign_key'] = $this->getModifierForeignKey();
        $result['col_modifier_name'] = $this->getModifierName();
        $result['col_mtime'] = $this->getMtime();
        $result['col_name'] = $this->getName();
        $result['col_foreign_key'] = $this->getForeignKey();
        $result['col_path_step'] = $this->getPathStep();
        $result['col_path'] = $this->getPath();
        $result['col_memo'] = $this->getMemo();
        $result['col_serial_format'] = $this->getSerialFormat();
        $result['col_serial_number'] = $this->getSerialNumber();
        $result['col_serial_type'] = $this->getSerialType();
        $result['col_transactor'] = $this->getTransactor();
        $result['col_transactor_name'] = $this->getTransactorName();
        $result['col_type'] = $this->getType();
        $result['col_admin_memo'] = $this->getAdminMemo();
        $result['col_auto_export'] = $this->getAutoExport();
        $result['col_deleted'] = $this->getDeleted();
        $result['col_export_folder'] = $this->getExportFolder();
        $result['col_icon_id'] = $this->getIconId();
        $result['col_icon_type'] = $this->getIconType();
        $result['col_icon_url'] = $this->getIconUrl();

        $result['col_category'] = $this->getCategoryId();
        $result['col_category_name'] = $this->getCategoryName();

        return $result;
    }

