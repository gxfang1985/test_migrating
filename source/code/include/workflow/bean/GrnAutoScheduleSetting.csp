<?php

class GrnAutoScheduleSetting
{
    private $_col_active;
    private $_col_form;
    private $_col_schedule_type;
    private $_col_appointment_type;
    private $_col_schedule_start_item;
    private $_col_schedule_end_item;
    private $_col_creator;
    private $_col_creator_foreign_key;
    private $_col_creator_name;
    private $_col_modifier;
    private $_col_modifier_foreign_key;
    private $_col_modifier_name;
    private $_col_ctime;
    private $_col_mtime;

    /**
     * @param null $inDataArray
     */
    public function __construct($inDataArray = null)
    {
        if (is_null($inDataArray)) {
            $this->setActive('');
            $this->setFormId('');
            $this->setScheduleType('');
            $this->setAppointmentType('');
            $this->setScheduleStartItem('');
            $this->setScheduleEndItem('');
            $this->setCreator('');
            $this->setCreatorForeignKey('');
            $this->setCreatorName('');
            $this->setCtime('');
            $this->setModifier('');
            $this->setModifierForeignKey('');
            $this->setModifierName('');
            $this->setMtime('');
        } else {
            $this->setActive(cb_at($inDataArray, 'col_active', ''));
            $this->setFormId(cb_at($inDataArray, 'col_form', ''));
            $this->setScheduleType(cb_at($inDataArray, 'col_schedule_type',
                ''));
            $this->setAppointmentType(cb_at($inDataArray,
                'col_appointment_type', ''));
            $this->setScheduleStartItem(cb_at($inDataArray,
                'col_schedule_start_item', ''));
            $this->setScheduleEndItem(cb_at($inDataArray,
                'col_schedule_end_item', ''));
            $this->setCreator(cb_at($inDataArray, 'col_creator', ''));
            $this->setCreatorForeignKey(cb_at($inDataArray,
                'col_creator_foreign_key', ''));
            $this->setCreatorName(cb_at($inDataArray, 'col_creator_name', ''));
            $this->setCtime(cb_at($inDataArray, 'col_ctime', ''));
            $this->setModifier(cb_at($inDataArray, 'col_modifier', ''));
            $this->setModifierForeignKey(cb_at($inDataArray,
                'col_modifier_foreign_key', ''));
            $this->setModifierName(cb_at($inDataArray, 'col_modifier_name',
                ''));
            $this->setMtime(cb_at($inDataArray, 'col_mtime', ''));
        }
    }

    /**
     * @return array
     */
    public function convertArray()
    {
        $result = [];

        $result['col_active'] = $this->getActive();
        $result['col_form'] = $this->getFormId();
        $result['col_schedule_type'] = $this->getScheduleType();
        $result['col_appointment_type'] = $this->getAppointmentType();
        $result['col_schedule_start_item'] = $this->getScheduleStartItem();
        $result['col_schedule_end_item'] = $this->getScheduleEndItem();
        $result['col_creator'] = $this->getCreator();
        $result['col_creator_foreign_key'] = $this->getCreatorForeignKey();
        $result['col_creator_name'] = $this->getCreatorName();
        $result['col_ctime'] = $this->getCtime();
        $result['col_modifier'] = $this->getModifier();
        $result['col_modifier_foreign_key'] = $this->getModifierForeignKey();
        $result['col_modifier_name'] = $this->getModifierName();
        $result['col_mtime'] = $this->getMtime();

        return $result;
    }

    public function getActive()
    {
        return $this->_col_active;
    }

    public function getFormId()
    {
        return $this->_col_form;
    }

    public function getScheduleType()
    {
        return $this->_col_schedule_type;
    }

    public function getAppointmentType()
    {
        return $this->_col_appointment_type;
    }

    public function getScheduleStartItem()
    {
        return $this->_col_schedule_start_item;
    }

    public function getScheduleEndItem()
    {
        return $this->_col_schedule_end_item;
    }

    public function getCreator()
    {
        return $this->_col_creator;
    }

    public function getCreatorName()
    {
        return $this->_col_creator_name;
    }

    public function getCreatorForeignKey()
    {
        return $this->_col_creator_foreign_key;
    }

    public function getModifier()
    {
        return $this->_col_modifier;
    }

    public function getModifierName()
    {
        return $this->_col_modifier_name;
    }

    public function getModifierForeignKey()
    {
        return $this->_col_modifier_foreign_key;
    }

    public function getCtime()
    {
        return $this->_col_ctime;
    }

    public function getMtime()
    {
        return $this->_col_mtime;
    }

    public function setActive($value)
    {
        $this->_col_active = $value;
    }

    public function setFormId($value)
    {
        $this->_col_form = $value;
    }

    public function setScheduleType($value)
    {
        $this->_col_schedule_type = $value;
    }

    public function setAppointmentType($value)
    {
        $this->_col_appointment_type = $value;
    }

    public function setScheduleStartItem($value)
    {
        $this->_col_schedule_start_item = $value;
    }

    public function setScheduleEndItem($value)
    {
        $this->_col_schedule_end_item = $value;
    }

    public function setCreator($value)
    {
        return $this->_col_creator = $value;
    }

    public function setCreatorName($value)
    {
        return $this->_col_creator_name = $value;
    }

    public function setCreatorForeignKey($value)
    {
        return $this->_col_creator_foreign_key = $value;
    }

    public function setModifier($value)
    {
        return $this->_col_modifier = $value;
    }

    public function setModifierName($value)
    {
        return $this->_col_modifier_name = $value;
    }

    public function setModifierForeignKey($value)
    {
        return $this->_col_modifier_foreign_key = $value;
    }

    public function setCtime($value)
    {
        return $this->_col_ctime = $value;
    }

    public function setMtime($value)
    {
        return $this->_col_mtime = $value;
    }
}
