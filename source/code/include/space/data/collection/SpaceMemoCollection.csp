<?php

namespace grn\space\data\collection;

use grn\space\common\data\bean\BeanAbstract;
use grn\space\common\data\collection\InspectionCollectionAbstract;

/**
 * Class SpaceMemoCollection
 *
 */
class SpaceMemoCollection extends InspectionCollectionAbstract
{
    /**
     * コンストラクタ
     */
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * 保持するBeanオブジェクト配列に、SpaceMemoオブジェクトを追加する
     *
     * @param BeanAbstract $bean Instance of {@link grn\space\data\bean\SpaceMemo}.
     */
    public function appendBean(BeanAbstract $bean)
    {
        assert('$bean instanceof grn\\space\\data\\bean\\SpaceMemo');
        parent::appendBean($bean);
    }
}
