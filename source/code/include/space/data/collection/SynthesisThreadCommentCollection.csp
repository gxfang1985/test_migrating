<?php

namespace grn\space\data\collection;

use grn\space\common\data\bean\BeanAbstract;
use grn\space\common\data\collection\ComponentInspectionCollectionAbstract;
use grn\space\common\data\bean\ContentInterface;

/**
 * * * * * * * * * * * *
 * Non-recommendation
 * A new program is not recommended to be made by using this class.
 * * * * * * * * * * * *
 *
 * SynthesisThreadCommentCollectionクラス
 **/
class SynthesisThreadCommentCollection
    extends ComponentInspectionCollectionAbstract
{
    /**
     * コンストラクタ
     */
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * 保持するBeanオブジェクト配列に、SynthesisThreadCommentを実装したオブジェクトを追加する
     *
     * @param     BeanAbstract $bean Object that implements {@link grn\space\data\bean\SynthesisThreadComment}.
     */
    public function appendBean(BeanAbstract $bean)
    {
        assert('$bean instanceof grn\\space\\data\\bean\\SynthesisThreadComment');
        parent::appendBean($bean);
    }
}

