<?php

namespace grn\space\data\collection;

use grn\space\common\data\collection\InspectionCollectionAbstract;
use grn\space\data\bean\Todo;
use grn\space\common\data\bean\BeanAbstract;

/**
 * TodoCollectionクラス
 **/
class TodoCollection extends InspectionCollectionAbstract
{
    /**
     * コンストラクタ
     */
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * 保持するBeanオブジェクト配列に、Todoオブジェクトを追加する
     *
     * @param     BeanAbstract $bean Object that implements Todo
     */
    public function appendBean(BeanAbstract $bean)
    {
        assert('$bean instanceof grn\space\data\bean\Todo');
        parent::appendBean($bean);
    }
}

