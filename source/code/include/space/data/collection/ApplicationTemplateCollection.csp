<?php

namespace grn\space\data\collection;

use grn\space\common\data\bean\BeanAbstract;
use grn\space\common\data\collection\InspectionCollectionAbstract;

/**
 * Class ApplicationTemplateCollection
 */
class ApplicationTemplateCollection extends InspectionCollectionAbstract
{
    /**
     * コンストラクタ
     */
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * 保持するBeanオブジェクト配列に、Threadオブジェクトを追加する
     *
     * @param     BeanAbstract $bean Object implements {@link grn\space\data\bean\ApplicationTemplate}.
     */
    public function appendBean(BeanAbstract $bean)
    {
        assert('$bean instanceof grn\\space\\data\\bean\\ApplicationTemplate');
        parent::appendBean($bean);
    }
}
