<?php

namespace grn\space\data\collection;

use grn\space\common\data\bean\BeanAbstract;
use grn\space\common\data\collection\BeanCollectionAbstract;

/**
 * DiscussionNonNotificationCollectionクラス
 **/
class DiscussionNonNotificationCollection extends BeanCollectionAbstract
{
    /**
     * コンストラクタ
     */
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * 保持するBeanオブジェクト配列に、DiscussionNonNotificationオブジェクトを追加する
     *
     * @param     BeanAbstract $bean Object that implements DiscussionNonNotification
     */
    public function appendBean(BeanAbstract $bean)
    {
        assert('$bean instanceof grn\\space\\data\\bean\\DiscussionNonNotification');
        parent::appendBean($bean);
    }
}
