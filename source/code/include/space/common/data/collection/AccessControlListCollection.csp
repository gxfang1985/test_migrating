<?php

namespace grn\space\common\data\collection;

use grn\space\common\data\bean\BeanAbstract;
use grn\space\common\data\collection\BeanCollectionAbstract;
use grn\space\common\data\bean\AccessControlList;

/**
 * AccessControlListCollectionクラス
 * アクセスコントロールリストの集合を表現するクラス
 */
class AccessControlListCollection extends BeanCollectionAbstract
{
    /**
     * コンストラクタ
     *
     */
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * 保持するBeanオブジェクト配列に、AccessControlListオブジェクトを追加する
     *
     * @param     BeanAbstract $bean Object that implements AccessControlList
     */
    public function appendBean(BeanAbstract $bean)
    {
        assert('is_subclass_of($bean, "grn\\space\\common\\data\\bean\\AccessControlList")
                || $bean instanceof grn\\space\\common\\data\\bean\\AccessControlList');
        parent::appendBean($bean);
    }
}

