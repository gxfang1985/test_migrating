<?php

if (isset($cbpapi_service)) {
    require_once('star/config.csp');
    $logic = GRN_Star_SystemLogic::getInstance();

    $star_num_allow = $logic->getStarLimit();

    require_once('grn/smarty.csp');
    $t = new GRN_Smarty;
    $t->assign('xml_namespaces', [grn_cbpapi_get_namespace('star')]);
    $t->assign('action', $cbpapi_service->getAction());
    $t->assign('star_num_allow', $star_num_allow);
    grn_cbpapi_response($t, __FILE__);
}


