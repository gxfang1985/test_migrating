<?php
if (isset($cbpapi_service)) {
    // get SOAP action
    $action = $cbpapi_service->getAction();

    //get TimezoneDB version of PHP on Garoon
    $timezone_version = timezone_version_get();

    //response SOAP message
    require_once("grn/smarty.csp");

    $t = new GRN_Smarty;
    $t->assign('xml_namespaces', [grn_cbpapi_get_namespace('base')]);
    $t->assign('action', $action);
    $t->assign('timezone_version', $timezone_version);

    grn_cbpapi_response($t, __FILE__);
}

