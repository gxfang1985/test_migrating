<?php

require_once('grn/smarty.csp');
$t = new GRN_Smarty;

require_once('grn/mime.csp');
$mm = GRN_MIMEConfigManager::getInstance();
$id = @$G_INPUT['id'];
$info =& $mm->getMIMETypeById($id);
if (false === $info) {
    cb_throw_error(E_GRN_MIME_MIMETYPE_NOT_FOUND);
}
$t->assign('id', $id);
$t->assign('info', $info);
include('_mime_1.csp');

$t->display(cb_get_pagename() . '.tpl');


