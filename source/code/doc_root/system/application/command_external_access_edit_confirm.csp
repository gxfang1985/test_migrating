<?php

use grn\grn\access\service\AppAvailabilityService;

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    $app_availability_service = new AppAvailabilityService();
    $is_required_logging = true;
    $app_availability_service->initializeTheRemoteAccessRule($is_required_logging);

    cb_redirect('system/application/external_access_detail');
}

