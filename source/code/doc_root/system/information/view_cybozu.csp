<?php
require_once('grn/rss.csp');
$cybozu = GRN_RSS_CybozuInfo::getInstance();

if ( ! array_key_exists('id', $G_INPUT)) {
    require_once('grn/error_code.csp');
    cb_throw_error(E_GRN_CMMN_RSS_CYBOZU_INFO_NOT_FOUND);
}

$uum =& $G_container_base->getInstance('uum');
$user =& $uum->getLoginUser();

$G_state_set->set('redirect_to', $cybozu->getItem($user, $G_INPUT['id']));
cb_safe_exit();

