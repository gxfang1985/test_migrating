<?php

$set = [];

if ($init === true) {
    require_once('grn/http_proxy.csp');
    $hpcm = GRN_HttpProxyConfigManager::getInstance();

    //////////////////
    //    初期値
    //////////////////
    $set['available'] = true;
    $set['port'] = 8080;

    $hpcm->getProperty('available', $set['available']);
    $hpcm->getProperty('server', $set['server']);
    $hpcm->getProperty('port', $set['port']);
    $hpcm->getProperty('exception_address', $set['exception_address']);
} else {
    $set = $G_INPUT;
}

$t->assign('set', $set);
$page_title = grn_get_current_page_display_name();
$t->assign('page_title', $page_title);
$t->assign('site_position', [['page' => '', 'name' => $page_title]]);


