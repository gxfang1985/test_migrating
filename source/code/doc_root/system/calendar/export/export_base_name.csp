<?php

global $G_INPUT;

require_once("grn/smarty.csp");
$t = new GRN_Smarty;

require_once('fw/i18n/locale.csp');
$languagesArray = CB_LanguageManager::getAvailableLanguages();
$t->assign('language_codes', $languagesArray);

$page_title = grn_get_current_page_display_name();
$t->assign('page_title', $page_title);

$t->assign('site_position', [
    [
        'page' => 'system/calendar/export/export_index',
        'name' => grn_get_page_display_name('system/calendar/export/export_index'),
    ],
    [
        'page' => '',
        'name' => $page_title,
    ],
]);

$t->display(cb_get_pagename() . ".tpl");
