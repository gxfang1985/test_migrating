<?php
require_once('memo/folder_tree.csp');
$class_name = 'GRN_Memo_FolderTree';
$oid = @$G_INPUT['oid'];
$page_name = @$G_INPUT['page'];
require_once('grn/org_tree.csp');
$util = GRN_OrgTreeUtil::getInstance();
$tree = $util->getTree($page_name, $class_name);
if ( ! $tree->containNode($oid)) {
    require_once('memo/folder_tree.csp');
    GRN_Memo_FolderTree::_memo_rebuild_folder_tree($oid);
}
include('../grn/_org_json_26.csp');

