<?php

use grn\favour\screen\mobile\FavourUserList;
use grn\grn\JSONResponse;

global $G_INPUT;
$userList = new FavourUserList($G_INPUT);

global $G_state_set;
$G_state_set->set('error_page_type', 'json');

$userList->setIsFromAjax(true);
