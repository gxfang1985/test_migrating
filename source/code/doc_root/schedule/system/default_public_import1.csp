<?php

global $G_INPUT;

require_once("grn/smarty.csp");
$smarty = new GRN_Smarty;

// SmartyValidate should be initialized when an input form is there.
require("SmartyValidate.class.php");
SmartyValidate::connect($smarty);
SmartyValidate::register_form(cb_get_pagename(), true);

include("_default_public_import1.csp");

if (array_key_exists("charset", $G_INPUT)) {
    $smarty->assign("charset", $G_INPUT["charset"]);
}
if (array_key_exists("skip", $G_INPUT)) {
    $smarty->assign("skip", $G_INPUT["skip"]);
}

// display smarty
$smarty->display(cb_get_pagename() . ".tpl");
