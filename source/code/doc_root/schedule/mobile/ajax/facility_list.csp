<?php

use grn\schedule\screen\mobile\ScheduleFacilityList;
use grn\grn\JSONResponse;

global $G_INPUT;
$facility_list = new ScheduleFacilityList($G_INPUT);

global $G_state_set;
$G_state_set->set('error_page_type', 'json');

