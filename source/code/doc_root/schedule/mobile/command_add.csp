<?php

use grn\schedule\screen\mobile\ScheduleAdd;
use grn\grn\mobile\MobileScreenCommon;
use grn\grn\JSONResponse;

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;

    $screen = new MobileScreenCommon(new ScheduleAdd($G_INPUT));
    $json = JSONResponse::create();
    $json->response($screen->post($G_INPUT));
}


