<?php

global $G_INPUT;
$group_id = array_key_exists('gid', $G_INPUT) ? $G_INPUT['gid'] : null;
if (strlen($group_id) > 0) {
    if (mb_substr($group_id, 0, 1) != 'f' && $group_id != 'selected') {
        $G_INPUT['gid'] = 'f';
    }
} else {
    $G_INPUT['gid'] = 'f';
}

cb_set_pagename('schedule/index');

include('index.csp');


