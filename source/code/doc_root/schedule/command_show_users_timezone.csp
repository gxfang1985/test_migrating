<?php
if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;
    $status = (isset($G_INPUT['status']) && $G_INPUT['status'] > 0) ? 1 : 0;
    $page = isset($G_INPUT['page']) ? $G_INPUT['page'] : '';

    require_once('schedule/personal_logic.csp');
    $logic = GRN_Schedule_PersonalLogic::getInstance();
    $logic->setShowUsersTimezone($logic->getLoginUser(), $status, $page);

}

