<?php

use grn\grn\mobile\MobileGroupUserList;
use grn\grn\JSONResponse;

global $G_INPUT;
$mobileGroupUserList = new MobileGroupUserList($G_INPUT);

global $G_state_set;
$G_state_set->set('error_page_type', 'json');

$mobileGroupUserList->setIsFromAjax(true);
$json = JSONResponse::create();
$json->response([
    'list'   => $mobileGroupUserList->fetch(),
    'offset' => $mobileGroupUserList->getNewOffset()
]);
