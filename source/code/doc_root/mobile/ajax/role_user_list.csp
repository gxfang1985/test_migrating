<?php

use grn\grn\mobile\MobileRoleUserList;
use grn\grn\JSONResponse;

global $G_INPUT;
$mobileRoleUserList = new MobileRoleUserList($G_INPUT);

global $G_state_set;
$G_state_set->set('error_page_type', 'json');

$mobileRoleUserList->setIsFromAjax(true);
$json = JSONResponse::create();
$json->response([
    'list'   => $mobileRoleUserList->fetch(),
    'offset' => $mobileRoleUserList->getNewOffset()
]);
