<?php

use grn\kunai\screen\SystemCommonSet;
use grn\kunai\screen\common\ScreenCommon;

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;

    $screen = new ScreenCommon(new SystemCommonSet($G_INPUT));
    $screen->post($G_INPUT);

    cb_redirect('system/application_list', ['app_id' => 'kunai']);
}
