<?php

use grn\kunai\screen\common\ScreenCommon;
use grn\kunai\screen\SystemAvailabilityUserImport2;

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;

    $screen = new ScreenCommon(new SystemAvailabilityUserImport2($G_INPUT));
    $result = $screen->post($G_INPUT);

    if (is_array($result)) {
        cb_redirect($result[0], $result[1]);
    }
}
