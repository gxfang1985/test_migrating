<?php

use grn\kunai\screen\SystemAvailabilityUserModify;
use grn\kunai\screen\common\ScreenCommon;

if (strcasecmp($_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;
    $screen = new ScreenCommon(new SystemAvailabilityUserModify($G_INPUT));
    $screen->post($G_INPUT);
    cb_redirect('kunai/system/availability_user_list');
}
