<?php

use grn\kunai\screen\common\ScreenCommon;
use grn\kunai\screen\SystemAvailabilityUserImport1;

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;

    $screen = new ScreenCommon(new SystemAvailabilityUserImport1($G_INPUT));
    $result = $screen->post($G_INPUT);

    if ($result == false) {
        echo $screen->fetch();
    } else {
        cb_redirect($result[0], $result[1]);
    }
}
