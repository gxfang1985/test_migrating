<?php

if (is_array(@ $G_INPUT['event_ids'])) {
    foreach ($G_INPUT['event_ids'] as $event_id) {
        switch ($event_id) {
            case 'Weather':
                require_once('cbwebsrv/weather.csp');
                $service = GRN_CBWebSrv_WeatherService::getInstance();
                break;
            case 'Rokuyo':
                require_once('cbwebsrv/six_kinds_of_day.csp');
                $service = GRN_CBWebSrv_SixKindsOfDayService::getInstance();
                break;
        }

        $service->getEventData();
    }
}

cb_redirect('system/application_list', ['app_id' => 'cbwebsrv']);


