<?php
require_once('grn/smarty.csp');
$t = new GRN_Smarty();

require_once('star/config.csp');
$logic = GRN_Star_SystemLogic::getInstance();

$star_limit = $logic->getStarLimit();
$star_limit_menu = [-1, 50, 100, 200, 300];
// Data for viewing
$t->assign('star_limit_menu', $star_limit_menu);
$t->assign('star_limit', $star_limit);

//Set page title
$page_title = grn_get_current_page_display_name();
$t->assign('page_title', $page_title);
//Set site position
$position[] = ['page' => "", 'name' => $page_title];
$t->assign('site_position', $position);
// set page for displaying
$t->display(cb_get_pagename() . '.tpl');
