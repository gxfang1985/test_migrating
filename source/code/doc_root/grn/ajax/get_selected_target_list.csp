<?php

use grn\grn\JSONResponse;
use grn\grn\customization\screen\AjaxSelectedTargetListScreen;

if (0 == strcasecmp(cb_at($_SERVER, 'REQUEST_METHOD'), 'POST')) {
    global $G_INPUT;

    $screen = new AjaxSelectedTargetListScreen($G_INPUT);

    $json = JSONResponse::create();
    $json->response(['content' => $screen->fetch()]);
}
