<?php

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;

    if (($eid = @ $G_INPUT['eid'])) {
        if ( ! ($folder_id = @ $G_INPUT['hid'])) {
            cb_throw_error(E_GRN_CABINET_FOLDER_NOT_FOUND);
        }

        foreach ($eid as $fid) {
            $G_cabinet->deleteFile($G_cabinet_login, $fid, $folder_id);
        }
    }
}

