<?php

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    $eid = cb_at($G_INPUT, 'eid');
    if ( ! is_array($eid)) {
        $params = ['nid' => cb_at($G_INPUT, 'nid')];
        cb_redirect('cabinet/system/privilege_list', $params);
    }

    require('../_command_privilege_delete_multi.csp');

    $params = ['nid' => cb_at($G_INPUT, 'nid')];
    cb_redirect('cabinet/system/privilege_list', $params);
}

