<?php

require_once("grn/smarty.csp");
$smarty = new GRN_Smarty;
$smarty->assign(
    "site_position",
    [
        [
            "page" => "space/system/export_index",
            "name" => grn_get_page_display_name("space/system/export_index")
        ],
        ["name" => grn_get_current_page_display_name()]
    ]
);
$smarty->assign("page_title", grn_get_current_page_display_name());

require_once('fw/i18n.csp');
$i18n = CB_I18N::getInstance();
$languagesArray = $i18n->getAvailableLanguages();
$smarty->assign('language_codes', $languagesArray);

$smarty->display(cb_get_pagename() . ".tpl");

