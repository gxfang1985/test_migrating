<?php

use grn\space\screen\mobile\application\todo\TodoDetail;

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    global $G_INPUT;
    $todo_complete = new TodoDetail($G_INPUT);

    global $G_state_set;
    $G_state_set->set('error_page_type', 'json');

    $todo_complete->post($G_INPUT);

    echo json_encode($todo_complete->fetchTodoUsers());
}
