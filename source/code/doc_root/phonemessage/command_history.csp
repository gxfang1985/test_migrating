<?php

if (strcasecmp(@ $_SERVER['REQUEST_METHOD'], 'POST') == 0) {
    // instantiate an Smarty object
    require_once("grn/smarty.csp");
    $t = new GRN_Smarty;

    $cmd = @$G_INPUT['cmd'];
    if ($cmd == 'group') {
        cb_redirect('phonemessage/index', ['gid' => @$G_INPUT['gid']]);
    } elseif ($cmd == 'delete') {
        cb_redirect('phonemessage/delete_multi', ['eid' => @$G_INPUT['eid']]);
    } else {
        cb_redirect('phonemessage/history',
            ['gid' => @$G_INPUT['gid'], 'cid' => @$G_INPUT['cid']]);
    }

}
