<?php

require_once('fw/error_handler.csp');

class plugin_fw_error_filter_json extends CB_ErrorMessageFilter
{
    /**
     * @param string $msg
     *
     * @return string
     */
    function filter($msg)
    {
        $msg = preg_replace("#<br[[:space:]]*/{0,1}>#i", "<br />\r\n", $msg);

        return strip_tags($msg);
    }
}


