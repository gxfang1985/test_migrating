<?php

require_once('fw/date.csp');

class plugin_fw_timezone_jst extends CB_TimezoneInfo
{
    function __construct()
    {
        parent::__construct('Asia/Tokyo');
    }

    function getName()
    {
        $name = parent::getName();
        if ($name == "JST") {
            $this->_name = "Asia/Tokyo";
        }

        return parent::getName();
    }
}


