<?php

class plugin_grn_common_system_06sso extends GRN_SystemLogicBase
{
    function __construct()
    {
        parent::__construct('sso');
    }

    function getConfigArray()
    {
        require_once('grn/license.csp');
        global $G_license_mode;
        if ($G_license_mode !== GRN_LICENSE_SUCCESS) {
            return false;
        }

        return [
            'general' => [
                [
                    'page' => 'system/sso/sso_method_list',
                    'icon' => 'common32'
                ]
            ]
        ];
    }
}


