<?php

class plugin_grn_common_system_11logging extends GRN_SystemLogicBase
{
    function __construct()
    {
        parent::__construct('logging');
    }

    function getConfigArray()
    {
        require_once('grn/license.csp');
        global $G_license_mode;
        if ($G_license_mode !== GRN_LICENSE_SUCCESS) {
            return false;
        }

        $ret = [
            'general' => [
                [
                    'page' => 'system/logging/admin',
                    'icon' => 'accesslog32'
                ],
            ]
        ];

        $ret['general'][] = [
            'page' => 'system/logging/common_set',
            'icon' => 'general32'
        ];

        return $ret;
    }
}


