<?php

class plugin_grn_common_system_95privilege extends GRN_SystemLogicBase
{
    function __construct()
    {
        parent::__construct('privilege');
    }

    function getConfigArray()
    {
        require_once('grn/license.csp');
        global $G_license_mode;
        if ($G_license_mode !== GRN_LICENSE_SUCCESS) {
            return false;
        }

        return [
            'general' => [
                [
                    'page' => 'system/privilege/common_admin_list',
                    'icon' => 'adminbasic32'
                ],
                [
                    'page' => 'system/privilege/application_admin_list',
                    'icon' => 'function32'
                ],
            ],
        ];
    }
}


