//Include Files
require_once('grn/authentication.csp');

//String Resources

function plugin_grn_common_authentication_application_cellular($page_parts)
{
    //Check Page Path
    if ($page_parts[0] != 'cellular') {
        return null;
    }

    //Get Driver Name
    $driver_name = null;
    switch ($page_parts[1]) {
        case 'system':
        case 'personal':
            $driver_name = null;
            break;
        default:
            $driver_name = 'cellular';
            break;
    }

    //Return Driver Name
    return $driver_name;
}


