<?xml version="1.0" ?>
<definitions name="CybozuReportServices"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             targetNamespace="http://wsdl.cybozu.co.jp/report/2008"
             xmlns:typens="http://wsdl.cybozu.co.jp/report/2008/types"
             xmlns:tns="http://wsdl.cybozu.co.jp/report/2008"
             xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/"
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
             xmlns:base_services="http://wsdl.cybozu.co.jp/base/2008">

  <wsdl:import namespace="http://wsdl.cybozu.co.jp/base/2008" location="base.wsdl"/>
  <types>
    <xsd:schema targetNamespace="http://wsdl.cybozu.co.jp/report/2008/types"
                xmlns="http://www.w3.org/2001/XMLSchema"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns:base="http://schemas.cybozu.co.jp/base/2008"
                xmlns:report="http://schemas.cybozu.co.jp/report/2008"
                xmlns:base_types="http://wsdl.cybozu.co.jp/base/2008/types">

      <xsd:import namespace="http://schemas.cybozu.co.jp/base/2008" schemaLocation="base.xsd" />
      <xsd:import namespace="http://schemas.cybozu.co.jp/report/2008" schemaLocation="report.xsd" />
      <xsd:import namespace="http://wsdl.cybozu.co.jp/base/2008/types" />

      <xsd:complexType name="ReportAddFollowType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="follow" type="report:FollowType"
                       minOccurs="1" maxOccurs="1" />
          <xsd:element name="file" minOccurs="0" maxOccurs="unbounded">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="content" type="xsd:base64Binary"
                             minOccurs="1" maxOccurs="1" />
              </xsd:sequence>
              <xsd:attribute name="id" type="base:IDType" use="required"/>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="report_id" type="base:IDType" use="required" />
      </xsd:complexType>

      <xsd:complexType name="ReportAddFollowsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="add_follow" type="typens:ReportAddFollowType" minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ReportAddFollowsResponseType">
        <xsd:sequence>
          <xsd:element name="report" type="report:ReportType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="ReportGetReportVersionsRequestType">
        <xsd:sequence>
          <xsd:element name="report_item" type="base_types:ItemVersionType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="start" type="xsd:dateTime" use="required" />
        <xsd:attribute name="end" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="target" type="report:SearchTargetType" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="ReportGetReportVersionsResponseType">
        <xsd:sequence>
          <xsd:element name="report_item" type="base_types:ItemVersionResultType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="ReportGetReportByIdRequestType">
        <xsd:sequence>
          <xsd:element name="report_id" type="base:IDType" minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ReportGetReportByIdResponseType">
        <xsd:sequence>
          <xsd:element name="report" type="report:ReportType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="ReportGetFollowsRequestType">
        <xsd:attribute name="report_id"  type="base:IDType" use="required"/>
        <xsd:attribute name="limit"  type="xsd:unsignedLong" use="required"/>
        <xsd:attribute name="offset"  type="xsd:unsignedLong" use="required"/>
      </xsd:complexType>
      <xsd:complexType name="ReportGetFollowsResponseType">
        <xsd:sequence>
          <xsd:element name="follows" type="report:FollowType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="ReportRemoveFollowsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="follow_id" type="base:IDType" minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="ReportRemoveReportsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="report_id" type="base:IDType" minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="ReportSearchReportsRequestType">
        <xsd:attribute name="target" type="report:SearchTargetType" use="required" />
        <xsd:attribute name="keyword" type="xsd:string" use="required" />
      </xsd:complexType>
      <xsd:complexType name="ReportSearchReportsResponseType">
        <xsd:sequence>
          <xsd:element name="report" type="report:ReportType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="ReportFileDownloadRequestType">
        <xsd:attribute name="file_id" type="base:IDType" use="required" />
      </xsd:complexType>
      <xsd:complexType name="ReportFileDownloadResponseType">
        <xsd:sequence>
          <xsd:element name="file" type="base_types:FileBodyType" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>
    </xsd:schema>
  </types>
  
  <message name="ReportAddFollowsRequest">
    <part name="parameters" type="typens:ReportAddFollowsRequestType" />
  </message>
  <message name="ReportAddFollowsResponse">
    <part name="returns" type="typens:ReportAddFollowsResponseType" />
  </message>

  <message name="ReportGetReportVersionsRequest">
    <part name="parameters" type="typens:ReportGetReportVersionsRequestType" />
  </message>
  <message name="ReportGetReportVersionsResponse">
