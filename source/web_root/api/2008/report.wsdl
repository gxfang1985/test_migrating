  <message name="ReportRemoveFollowsRequest">
    <part name="parameters" type="typens:ReportRemoveFollowsRequestType" />
  </message>

  <message name="ReportRemoveReportsRequest">
    <part name="parameters" type="typens:ReportRemoveReportsRequestType" />
  </message>

  <message name="ReportSearchReportsRequest">
    <part name="parameters" type="typens:ReportSearchReportsRequestType" />
  </message>
  <message name="ReportSearchReportsResponse">
    <part name="returns" type="typens:ReportSearchReportsResponseType" />
  </message>

  <message name="ReportFileDownloadRequest">
