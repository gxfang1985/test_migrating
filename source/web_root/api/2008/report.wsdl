      <input message="tns:ReportGetFollowsRequest" />
      <output message="tns:ReportGetFollowsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="ReportRemoveFollows">
      <documentation>
      </documentation>
      <input message="tns:ReportRemoveFollowsRequest" />
      <output message="base_services:EmptyResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="ReportRemoveReports">
      <documentation>
      </documentation>
      <input message="tns:ReportRemoveReportsRequest" />
      <output message="base_services:EmptyResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="ReportSearchReports">
      <documentation>
      </documentation>
      <input message="tns:ReportSearchReportsRequest" />
      <output message="tns:ReportSearchReportsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="ReportFileDownload">
      <documentation>
      </documentation>
      <input message="tns:ReportFileDownloadRequest" />
      <output message="tns:ReportFileDownloadResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
  </portType>
  
  <binding name="ReportBinding" type="tns:ReportPort">
    <soap12:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http" />

    <operation name="ReportGetReportVersions">
      <soap12:operation soapAction="ReportGetReportVersions" soapActionRequired="true" />
      <input>
        <soap12:body use="literal" namespace="http://wsdl.cybozu.co.jp/report/2008" />
      </input>
      <output>
        <soap12:body use="literal" namespace="http://wsdl.cybozu.co.jp/report/2008" />
      </output>
      <fault name="CybozuError">
        <soap12:body use="literal" namespace="http://wsdl.cybozu.co.jp/report/2008" />
      </fault>
    </operation>

    <operation name="ReportGetReportById">
      <soap12:operation soapAction="ReportGetReportById" soapActionRequired="true" />
      <input>
        <soap12:body use="literal" namespace="http://wsdl.cybozu.co.jp/report/2008" />
      </input>
      <output>
