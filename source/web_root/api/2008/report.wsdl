  </message>
  <message name="ReportFileDownloadResponse">
    <part name="returns" type="typens:ReportFileDownloadResponseType" />
  </message>

  <portType name="ReportPort">
    <operation name="ReportGetReportVersions">
      <documentation>
      </documentation>
      <input message="tns:ReportGetReportVersionsRequest" />
      <output message="tns:ReportGetReportVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="ReportGetReportById">
      <documentation>
      </documentation>
      <input message="tns:ReportGetReportByIdRequest" />
      <output message="tns:ReportGetReportByIdResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="ReportAddFollows">
      <documentation>
      </documentation>
      <input message="tns:ReportAddFollowsRequest" />
      <output message="tns:ReportAddFollowsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="ReportGetFollows">
      <documentation>
