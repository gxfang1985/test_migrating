  <portType name="AddressPort">

    <operation name="AddressGetPersonalCardVersions">
      <documentation>
        This API gets the modified personal address cards.
        "card_item" is a list of address card's that a client has.
      </documentation>
      <input message="tns:AddressGetPersonalCardVersionsRequest" />
      <output message="tns:AddressGetPersonalCardVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="AddressGetPersonalCardsById">
      <documentation>
        This API gets the personal address cards by card id.
        The client sends list of "card_id" for specify the card to search.
      </documentation>
      <input message="tns:AddressGetPersonalCardsByIdRequest" />
      <output message="tns:AddressGetPersonalCardsByIdResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="AddressGetSharedCardVersions">
      <documentation>
        This API gets the modified address cards in shared books.
        "card_item" is a list of address card's that a client has.
      </documentation>
      <input message="tns:AddressGetSharedCardVersionsRequest" />
      <output message="tns:AddressGetSharedCardVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="AddressGetSharedCardsById">
      <documentation>
        This API gets the address cards by card id.
        The client sends list of "card_id" for specify the card to search.
      </documentation>
      <input message="tns:AddressGetSharedCardsByIdRequest" />
      <output message="tns:AddressGetSharedCardsByIdResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="AddressGetPersonalBookVersions">
      <documentation>
        This API gets the modified personal address books.
        "book_item" is a list of address book's that a client has.
      </documentation>
      <input message="tns:AddressGetPersonalBookVersionsRequest" />
      <output message="tns:AddressGetPersonalBookVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="AddressGetPersonalBooksById">
      <documentation>
        This API gets the personal address books by book id.
        The client sends list of "book_id" for specify the book to search.
      </documentation>
