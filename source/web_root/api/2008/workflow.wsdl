          <xsd:element name="file" type="base_types:FileBodyType"
                       minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetRequestsRequestType">
        <xsd:sequence>
          <xsd:element name="manage_request_parameter" type="typens:WorkflowGetRequestType"
                       minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetRequestsResponseType">
       <xsd:sequence>
          <xsd:element name="category" type="workflow:RequestManageFormType"
                       minOccurs="0" maxOccurs="unbounded" />
       </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetRequestByIdRequestType">
          <xsd:sequence>
            <xsd:element name="request_id" type="base:IDType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetRequestByIdResponseType">
       <xsd:sequence>
          <xsd:element name="application" type="typens:WorkflowApplicationType"
                       minOccurs="0" maxOccurs="unbounded" />
