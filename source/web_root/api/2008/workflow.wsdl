      <xsd:complexType name="WorkflowGetAvailabilityUsersResponseType">
        <xsd:sequence>
            <xsd:element name="user" type="typens:WorkflowAvailabilityUserType"
                         minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetAttachedFileBodyRequestType">
        <xsd:sequence>
            <xsd:element name="parameter" type="typens:WorkflowGetAttachedFileBodyType"
                         minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetAttachedFileBodyResponseType">
        <xsd:sequence>
            <xsd:element name="attachment_details" type="workflow:FileAttachedDetail"
                         minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetListCategoryRequestType">
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetListCategoryResponseType">
        <xsd:sequence>
            <xsd:element name="root" type="workflow:CategoriesType"
                         minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
       <xsd:complexType name="WorkflowGetRequestFormByCategoryIdsRequestType">
        <xsd:sequence>
            <xsd:element name="category_id" type="base:IDType"
                         minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetRequestFormByCategoryIdsResponseType">
        <xsd:sequence>
            <xsd:element name="category" type="workflow:RequestFormType"
                         minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
    </xsd:schema>
  </types>

