  <portType name="WorkflowPort">

    <operation name="WorkflowGetUnprocessedApplicationVersions">
      <documentation>
        This API gets the modified unprocessed workflow applications.
        "application_item" is a list of workflow applications that a client has.
      </documentation>
      <input message="tns:WorkflowGetUnprocessedApplicationVersionsRequest" />
      <output message="tns:WorkflowGetUnprocessedApplicationVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetUnprocessedApplicationsById">
      <documentation>
        This API gets the unprocessed workflow applications by application id.
        The client sends list of "application_id" for specify the application to search.
      </documentation>
      <input message="tns:WorkflowGetUnprocessedApplicationsByIdRequest" />
      <output message="tns:WorkflowGetUnprocessedApplicationsByIdResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetSentApplicationVersions">
      <documentation>
        This API gets the modified sent workflow applications.
        "application_item" is a list of workflow applications that a client has.
      </documentation>
      <input message="tns:WorkflowGetSentApplicationVersionsRequest" />
      <output message="tns:WorkflowGetSentApplicationVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetSentApplicationsById">
      <documentation>
        This API gets the sent workflow applications by application id.
        The client sends list of "application_id" for specify the application to search.
      </documentation>
      <input message="tns:WorkflowGetSentApplicationsByIdRequest" />
      <output message="tns:WorkflowGetSentApplicationsByIdResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetReceivedApplicationVersions">
      <documentation>
        This API gets the modified received workflow applications.
        "application_item" is a list of workflow applications that a client has.
      </documentation>
      <input message="tns:WorkflowGetReceivedApplicationVersionsRequest" />
      <output message="tns:WorkflowGetReceivedApplicationVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetReceivedApplicationsById">
      <documentation>
        This API gets the received workflow applications by application id.
        The client sends list of "application_id" for specify the application to search.
      </documentation>
      <input message="tns:WorkflowGetReceivedApplicationsByIdRequest" />
      <output message="tns:WorkflowGetReceivedApplicationsByIdResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetApprovalDelegators">
      <documentation>
        This API gets the users that has delegated to login user.
        The service returns list of the delegate user id.
      </documentation>
      <input message="tns:WorkflowGetApprovalDelegatorsRequest" />
      <output message="tns:WorkflowGetApprovalDelegatorsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetProxyApprovalsByDelegatorId">
      <documentation>
        This API gets the proxy approvals by delegator's user id.
        "start" and "end" are search range.
        This API gets applications where the applied datetime is included within the range.
      </documentation>
      <input message="tns:WorkflowGetProxyApprovalsByDelegatorIdRequest" />
      <output message="tns:WorkflowGetProxyApprovalsByDelegatorIdResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetPendingApprovals">
      <documentation>
        This API gets the pending approvals.
        "start" and "end" are search range.
        This API gets applications where the applied datetime is included within the range.
      </documentation>
      <input message="tns:WorkflowGetPendingApprovalsRequest" />
      <output message="tns:WorkflowGetPendingApprovalsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowHandleApplications">
      <documentation>
        This API handles the workflow applications.
        A possible operation is included in ApplicationType.
        The service returns the workflow application that handled.
      </documentation>
      <input message="tns:WorkflowHandleApplicationsRequest" />
      <output message="tns:WorkflowHandleApplicationsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="WorkflowGetProxies">
      <documentation>
        This API gets proxy users for login user.
      </documentation>
