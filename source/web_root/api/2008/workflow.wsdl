        <xsd:attribute name="use_pending_approvals" type="xsd:boolean" use="required" />
        <xsd:attribute name="use_proxy_approvals" type="xsd:boolean" use="required" />
        <xsd:attribute name="modify_proxies" type="xsd:boolean" use="required" />
      </xsd:complexType>

      <xsd:complexType name="WorkflowFileDownloadRequestType">
        <xsd:attribute name="file_id" type="base:IDType" use="required" />
      </xsd:complexType>
      <xsd:complexType name="WorkflowFileDownloadResponseType">
