<?xml version="1.0" ?>
<definitions name="CybozuWorkflowServices"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             targetNamespace="http://wsdl.cybozu.co.jp/workflow/2008"
             xmlns:typens="http://wsdl.cybozu.co.jp/workflow/2008/types"
             xmlns:tns="http://wsdl.cybozu.co.jp/workflow/2008"
             xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/"
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
             xmlns:base_services="http://wsdl.cybozu.co.jp/base/2008">

  <wsdl:import namespace="http://wsdl.cybozu.co.jp/base/2008" location="base.wsdl"/>

  <types>
    <xsd:schema targetNamespace="http://wsdl.cybozu.co.jp/workflow/2008/types"
                xmlns="http://www.w3.org/2001/XMLSchema"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns:base="http://schemas.cybozu.co.jp/base/2008"
                xmlns:workflow="http://schemas.cybozu.co.jp/workflow/2008"
                xmlns:base_types="http://wsdl.cybozu.co.jp/base/2008/types">

      <xsd:import namespace="http://schemas.cybozu.co.jp/base/2008"
                  schemaLocation="base.xsd" />
      <xsd:import namespace="http://schemas.cybozu.co.jp/workflow/2008"
                  schemaLocation="workflow.xsd" />
      <xsd:import namespace="http://wsdl.cybozu.co.jp/base/2008/types" />

      <xsd:simpleType name="WorkflowFolderType">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="unprocessed" />
          <xsd:enumeration value="sent" />
          <xsd:enumeration value="received" />
          <xsd:enumeration value="draft" />
          <xsd:enumeration value="finish" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="WorkflowStatusType">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="unprocessing" />
          <xsd:enumeration value="inprogress" />
          <xsd:enumeration value="sent_back" />
          <xsd:enumeration value="approved" />
          <xsd:enumeration value="rejected" />
          <xsd:enumeration value="withdrawn" />
          <xsd:enumeration value="cancelled" />
          <xsd:enumeration value="completed" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="WorkflowApplicationType">
        <xsd:complexContent>
          <xsd:extension base="workflow:ApplicationType">
            <xsd:sequence>
              <xsd:element name="folder_type" type="typens:WorkflowFolderType"
                           minOccurs="0" maxOccurs="unbounded" />
            </xsd:sequence>
            <xsd:attribute name="status_type" type="typens:WorkflowStatusType" use="required" />                           
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>

       <xsd:simpleType name="WorkflowGetManageRequestFilter">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="All" />
          <xsd:enumeration value="In progress" />
          <xsd:enumeration value="Approved" />
          <xsd:enumeration value="Rejected" />
          <xsd:enumeration value="Cancel" />
          <xsd:enumeration value="Complete" />
        </xsd:restriction>
      </xsd:simpleType>
      
      <xsd:complexType name="WorkflowAvailabilityUserType">
        <xsd:attribute name="id" type="base:IDType" use="required" />
        <xsd:attribute name="workflow" type="xsd:boolean" use="required" />
        <xsd:attribute name="keitai" type="xsd:boolean" use="required" />
      </xsd:complexType>
      
       <xsd:complexType name="WorkflowGetRequestType">
        <xsd:attribute name="request_form_id" type="base:IDType" use="required" />
        <xsd:attribute name="filter" type="typens:WorkflowGetManageRequestFilter" use="optional" />
        <xsd:attribute name="start_request_date" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="end_request_date" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="start_approval_date" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="end_approval_date" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="applicant" type="base:IDType" use="optional" />
        <xsd:attribute name="last_approval" type="base:IDType" use="optional" />
        <xsd:attribute name="start_to_get_information_from" type="base:IDType" use="optional" />
        <xsd:attribute name="maximum_request_amount_to_get" type="xsd:string" use="optional" />
        <xsd:anyAttribute processContents="lax" />
      </xsd:complexType>
      
      <xsd:complexType name="WorkflowGetAttachedFileBodyType">
        <xsd:attribute name="request_form_id" type="base:IDType" use="required" />
        <xsd:attribute name="file_id" type="base:IDType" use="required" />
        <xsd:anyAttribute processContents="lax" />
      </xsd:complexType>

