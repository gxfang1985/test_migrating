      <xsd:complexType name="BulletinRemoveTopicsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="topics" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
