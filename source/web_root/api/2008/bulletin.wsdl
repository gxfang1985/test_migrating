  <!-- *** portType *** -->
 <portType name="BulletinPort">
    
    <operation name="BulletinGetCategoryVersions">
      <documentation>
        This API gets all categories in the bulletin application.
      </documentation>
      <input message="tns:BulletinGetCategoryVersionsRequest" />
      <output message="tns:BulletinGetCategoryVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinGetCategories">
      <documentation>
        This API gets all categories in the bulletin application.
      </documentation>
      <input message="tns:BulletinGetCategoriesRequest" />
      <output message="tns:BulletinGetCategoriesResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinGetTopicVersions">
      <input message="tns:BulletinGetTopicVersionsRequest" />
      <output message="tns:BulletinGetTopicVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinGetDraftTopicVersions">
      <input message="tns:BulletinGetDraftTopicVersionsRequest" />
      <output message="tns:BulletinGetDraftTopicVersionsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinGetTopics">
        <documentation>
        This API gets topics list by "category_id".
        Exception 
        In case "category_id" = -1 return topics list in "Waiting to be published" category
        In case "category_id" = -2 return topics list in "Draft" category
        </documentation>
      <input message="tns:BulletinGetTopicsRequest" />
      <output message="tns:BulletinGetTopicsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinGetTopicByIds">
      <documentation>
        This API gets a topic by id.
        The client sends list of "topic_id" for specify the topic to retrieve topic
      </documentation>
      <input message="tns:BulletinGetTopicByIdsRequest" />
      <output message="tns:BulletinGetTopicByIdsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinFileDownload">
      <input message="tns:BulletinFileDownloadRequest" />
      <output message="tns:BulletinFileDownloadResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinCreateTopics">
      <input message="tns:BulletinCreateTopicsRequest" />
      <output message="tns:BulletinCreateTopicsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="BulletinModifyTopics">
      <documentation>
        This API modifies the Topics.
        The service returns the topics that modified.
      </documentation>
      <input message="tns:BulletinModifyTopicsRequest" />
      <output message="tns:BulletinModifyTopicsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="BulletinSaveDraftTopics">
      <documentation>
        This API can save new or modify topic to the draft.
        The service returns the draft topics that saved.
      </documentation>
      <input message="tns:BulletinSaveDraftTopicsRequest" />
      <output message="tns:BulletinSaveDraftTopicsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

     <operation name="BulletinRemoveTopics">
      <documentation>
        This API removes the topics.
        The service returns empty response.
      </documentation>
      <input message="tns:BulletinRemoveTopicsRequest" />
      <output message="base_services:EmptyResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinSearchTopics">
      <documentation>
        This API searches the topics.
      </documentation>
      <input message="tns:BulletinSearchTopicsRequest" />
      <output message="tns:BulletinSearchTopicsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="BulletinGetFollows">
      <documentation>
        This API gets follows from topic.
        "offset" and "limit" specifies search range.
      </documentation>
      <input message="tns:BulletinGetFollowsRequest" />
      <output message="tns:BulletinGetFollowsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
    <operation name="BulletinAddFollows">
      <documentation>
        This API add follows to topic.
        The service returns the topics that a follow added.
      </documentation>
      <input message="tns:BulletinAddFollowsRequest" />
      <output message="tns:BulletinAddFollowsResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>

    <operation name="BulletinRemoveFollows">
      <documentation>
        This API remove follows from topic.
        The service returns empty response.
      </documentation>
      <input message="tns:BulletinRemoveFollowsRequest" />
      <output message="base_services:EmptyResponse" />
      <fault name="CybozuError" message="base_services:Fault" />
    </operation>
    
  </portType>
  
