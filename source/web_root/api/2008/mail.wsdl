<?xml version="1.0" ?>
<definitions name="CybozuMailServices"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             targetNamespace="http://wsdl.cybozu.co.jp/mail/2008"
             xmlns:typens="http://wsdl.cybozu.co.jp/mail/2008/types"
             xmlns:tns="http://wsdl.cybozu.co.jp/mail/2008"
             xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/"
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
             xmlns:base_services="http://wsdl.cybozu.co.jp/base/2008">

  <wsdl:import namespace="http://wsdl.cybozu.co.jp/base/2008" location="base.wsdl"/>

  <types>
    <xsd:schema targetNamespace="http://wsdl.cybozu.co.jp/mail/2008/types"
                xmlns="http://www.w3.org/2001/XMLSchema"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns:base="http://schemas.cybozu.co.jp/base/2008"
                xmlns:mail="http://schemas.cybozu.co.jp/mail/2008"
                xmlns:base_types="http://wsdl.cybozu.co.jp/base/2008/types">

      <xsd:import namespace="http://schemas.cybozu.co.jp/base/2008"
                  schemaLocation="base.xsd" />
      <xsd:import namespace="http://schemas.cybozu.co.jp/mail/2008"
                  schemaLocation="mail.xsd" />
      <xsd:import namespace="http://wsdl.cybozu.co.jp/base/2008/types" />
      
      <xsd:complexType name="MailGetAccountVersionsRequestType">
        <xsd:sequence>
          <xsd:element name="account_item" type="base_types:ItemVersionType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailGetAccountVersionsResponseType">
        <xsd:sequence>
          <xsd:element name="account_item" type="base_types:ItemVersionResultType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailGetAccountsByIdRequestType">
        <xsd:sequence>
          <xsd:element name="account_id" type="base:IDType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailGetAccountsByIdResponseType">
        <xsd:sequence>
          <xsd:element name="account" type="mail:AccountType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailGetMailVersionsRequestType">
        <xsd:sequence>
          <xsd:element name="mail_item" type="base_types:ItemVersionType"
                       minOccurs="0" maxOccurs="unbounded" />
          <xsd:element name="folder_id" type="base:IDType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="start" type="xsd:dateTime" use="required" />
        <xsd:attribute name="end" type="xsd:dateTime" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="MailGetMailVersionsResponseType">
        <xsd:sequence>
          <xsd:element name="mail_item" type="base_types:ItemVersionResultType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailGetMailsByIdRequestType">
        <xsd:sequence>
          <xsd:element name="mail_id" type="base:IDType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailGetMailsByIdResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailSendMailType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="1" maxOccurs="1" />
          <xsd:element name="file" minOccurs="0" maxOccurs="unbounded">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="content" type="xsd:base64Binary"
                             minOccurs="1" maxOccurs="1" />
              </xsd:sequence>
              <xsd:attribute name="id" type="base:IDType" />
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="remove_file_id" type="base:IDType"
                         minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="account_id" type="base:IDType" use="required" />
        <xsd:attribute name="from_string" type="base:NonBlankStringType" use="optional" />
        <xsd:attribute name="sender_string" type="base:NonBlankStringType" use="optional" />
        <xsd:attribute name="to_string" type="base:NonBlankStringType" use="optional" />
        <xsd:attribute name="cc_string" type="base:NonBlankStringType" use="optional" />
        <xsd:attribute name="bcc_string" type="base:NonBlankStringType" use="optional" />
        <xsd:attribute name="reply_to_string" type="base:NonBlankStringType" use="optional" />
        <xsd:attribute name="draft_id" type="base:IDType" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="MailSendMailsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="send_mail" type="typens:MailSendMailType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailSendMailsResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailReplyMailsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="reply_mail" type="typens:MailSendMailType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="reply_all" type="xsd:boolean" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailReplyMailsResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailForwardMailType">
        <xsd:complexContent>
          <xsd:extension base="typens:MailSendMailType">
            <xsd:attribute name="mail_id" type="base:IDType" use="required" />
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="MailForwardMailsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="forward_mail" type="typens:MailForwardMailType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailForwardMailsResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:simpleType name="MailDraftOperationType">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="send" />
          <xsd:enumeration value="reply" />
          <xsd:enumeration value="reply_all" />
          <xsd:enumeration value="forward" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="MailDraftMailType">
        <xsd:complexContent>
          <xsd:extension base="typens:MailSendMailType">
            <xsd:attribute name="operation" type="typens:MailDraftOperationType" use="optional" />
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="MailSaveDraftMailsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="save_mail" type="typens:MailDraftMailType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailSaveDraftMailsResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailRemoveMailsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="mail_id" type="base:IDType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailSearchMailsRequestType">
        <xsd:attribute name="text" type="xsd:string" use="required" />
        <xsd:attribute name="start" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="end" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="search_all_accounts" type="xsd:boolean" use="required" />
        <xsd:attribute name="account_id" type="base:IDType" use="optional" />
        <xsd:attribute name="folder_id" type="base:IDType" use="optional" />
        <xsd:attribute name="search_sub_folders" type="xsd:boolean" use="optional" />
        <xsd:attribute name="title_search" type="xsd:boolean" use="required" />
        <xsd:attribute name="body_search" type="xsd:boolean" use="required" />
        <xsd:attribute name="from_search" type="xsd:boolean" use="required" />
        <xsd:attribute name="to_search" type="xsd:boolean" use="required" />
        <xsd:attribute name="cc_search" type="xsd:boolean" use="required" />
        <xsd:attribute name="bcc_search" type="xsd:boolean" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailSearchMailsResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:simpleType name="MailDispositionNotificationType">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="open" />
          <xsd:enumeration value="ignore" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="MailOpenDispositionNotificationOperationType">
        <xsd:attribute name="account_id" type="base:IDType" use="required" />
        <xsd:attribute name="mail_id" type="base:IDType" use="required" />
        <xsd:attribute name="type" type="typens:MailDispositionNotificationType" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailOpenDispositionNotificationsRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="operation" type="typens:MailOpenDispositionNotificationOperationType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailOpenDispositionNotificationsResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailModifyFolderOperationType">
        <xsd:sequence>
          <xsd:element name="folder" type="mail:FolderType"
                       minOccurs="1" maxOccurs="1" />
        </xsd:sequence>
        <xsd:attribute name="account_id" type="base:IDType" use="required" />
        <xsd:attribute name="parent_folder_id" type="base:IDType" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="MailAddFoldersRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="add_folder" type="typens:MailModifyFolderOperationType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailAddFoldersResponseType">
        <xsd:sequence>
          <xsd:element name="folder" type="mail:FolderType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailModifyFoldersRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="modify_folder" type="typens:MailModifyFolderOperationType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailModifyFoldersResponseType">
        <xsd:sequence>
          <xsd:element name="folder" type="mail:FolderType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailRemoveFoldersRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="folder_id" type="base:IDType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailMoveMailsOperationType">
        <xsd:attribute name="folder_id" type="base:IDType" />
        <xsd:attribute name="mail_id" type="base:IDType" />
      </xsd:complexType>
      <xsd:complexType name="MailMoveMailsToOtherFolderRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="operation" type="typens:MailMoveMailsOperationType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailMoveMailsToOtherFolderResponseType">
        <xsd:sequence>
          <xsd:element name="mail" type="mail:MailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailSignatureType">
        <xsd:attribute name="account_id" type="base:IDType" use="required" />
        <xsd:attribute name="name" type="base:NonBlankStringType" use="required" />
        <xsd:attribute name="content" type="xsd:string" use="required" />        
      </xsd:complexType>
      <xsd:complexType name="MailGetSignaturesRequestType">
        <xsd:attribute name="account_id" type="base:IDType" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailGetSignaturesResponseType">
        <xsd:sequence>
          <xsd:element name="signature" type="typens:MailSignatureType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailGetFiltersRequestType">
        <xsd:attribute name="account_id" type="base:IDType" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailGetFiltersResponseType">
        <xsd:sequence>
          <xsd:element name="filter" type="mail:FilterType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailPersonalProfileType">
        <xsd:sequence>
          <xsd:element name="from_name" minOccurs="0" maxOccurs="unbounded">
            <xsd:complexType>
              <xsd:attribute name="account_id" type="base:IDType" use="required" />
              <xsd:attribute name="name" type="base:NonBlankStringType" use="required" />
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="show_preview" type="xsd:boolean" use="required" />
        <xsd:attribute name="send_charset" type="xsd:string" use="required" />
        <xsd:attribute name="use_trash" type="xsd:boolean" use="required" />
        <xsd:attribute name="send_vcard" type="xsd:boolean" use="optional" />
        <xsd:attribute name="wrap" type="xsd:boolean" default="false" use="optional" />
        <xsd:attribute name="linewidth" type="xsd:positiveInteger" default="70" use="optional" />
        <xsd:attribute name="use_message_disposition_notification" type="xsd:boolean" use="required" />
        <xsd:attribute name="use_status" type="xsd:boolean" use="optional" />
        <xsd:attribute name="use_history" type="xsd:boolean" use="optional" />
        <xsd:attribute name="reply_message_disposition_notification" use="required">
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="None" />
              <xsd:enumeration value="ShowConfirmMessage" />
              <xsd:enumeration value="ReplyAlways" />
              <xsd:enumeration value="ReplyIncludingToOrCC" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:complexType>

      <xsd:complexType name="MailSizeLimitsType">
        <xsd:attribute name="total_kb" type="xsd:positiveInteger" use="optional" />
        <xsd:attribute name="receive_kb" type="xsd:positiveInteger" use="optional" />
        <xsd:attribute name="send_kb" type="xsd:positiveInteger" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="UserAuthoritiesType">
        <xsd:attribute name="allow_account_all_permission" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_account_modification" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_store_on_server" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_new_mail_check" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_collective_reception" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_send_markup_body" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_display_markup_image" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_message_disposition_notification" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_status" type="xsd:boolean" use="required" />
        <xsd:attribute name="allow_history" type="xsd:boolean" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailSystemProfileType">
        <xsd:sequence>
          <xsd:element name="auto_receive">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="receive_time" type="xsd:time" minOccurs="0" maxOccurs="unbounded" />
              </xsd:sequence>
              <xsd:attribute name="interval" type="xsd:duration" use="optional" />
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="limit" type="typens:MailSizeLimitsType"
                       minOccurs="1" maxOccurs="1" />
          <xsd:element name="authority" type="typens:UserAuthoritiesType"
                       minOccurs="1" maxOccurs="1" />          
        </xsd:sequence>
        <xsd:attribute name="disable_client" type="xsd:boolean" use="required" />
        <xsd:attribute name="check_new_mail_at_login" type="xsd:boolean" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailGetProfilesRequestType">
        <xsd:attribute name="include_system_profile" type="xsd:boolean" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="MailGetProfilesResponseType">
        <xsd:sequence>
          <xsd:element name="personal_profile" type="typens:MailPersonalProfileType"
                       minOccurs="1" maxOccurs="1"/>
          <xsd:element name="system_profile" type="typens:MailSystemProfileType"
                       minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="MailSetProfilesRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="personal_profile" type="typens:MailPersonalProfileType"
                       minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailSetProfilesResponseType">
        <xsd:sequence>
          <xsd:element name="personal_profile" type="typens:MailPersonalProfileType"
                       minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailSourceDownloadRequestType">
        <xsd:attribute name="mail_id" type="base:IDType" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailSourceDownloadResponseType">
        <xsd:sequence>
          <xsd:element name="source" type="base_types:FileBodyType"
                       minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="MailFileDownloadRequestType">
        <xsd:attribute name="mail_id" type="base:IDType" use="required" />
        <xsd:attribute name="file_id" type="base:IDType" use="required" />
      </xsd:complexType>
      <xsd:complexType name="MailFileDownloadResponseType">
        <xsd:sequence>
          <xsd:element name="file" type="base_types:FileBodyType"
                       minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="MailUserAccountType">
        <xsd:sequence>
              <xsd:element name="account_info" type="mail:Account_Info" minOccurs="0" maxOccurs="1" />
              <xsd:element name="mail_setting" type="mail:MailSetting" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailCreateUserAccountRequestType">
      <xsd:sequence>
        <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
        <xsd:element name="mail_user_accounts" type="typens:MailUserAccountType" minOccurs="0" maxOccurs="unbounded" />
      </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailCreateUserAccountResponseType">
        <xsd:sequence>
          <xsd:element name="user_accounts" type="typens:MailUserAccountType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="acc_id" type="base:IDType" use="required" />
      </xsd:complexType>
      
      <xsd:complexType name="MailEditUserAccountRequestType">
      <xsd:sequence>
        <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
        <xsd:element name="edit_user_accounts" type="typens:MailUserAccountType" minOccurs="0" maxOccurs="unbounded" />
      </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailEditUserAccountResponseType">
        <xsd:sequence>
          <xsd:element name="edit_user_accounts" type="typens:MailUserAccountType" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute name="acc_id" type="base:IDType" use="required" />
      </xsd:complexType>
      
      <xsd:complexType name="MailDeleteUserAccountRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="delete_user_accounts" type="mail:DeleteUserAccount" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="NewArrivingEmailType">
        <xsd:attribute name="id" type="base:IDType" use="required" />
        <xsd:attribute name="name" type="xsd:string" use="required" />
        <xsd:attribute name="email" type="xsd:string" use="required" />
        <xsd:attribute name="new_mails" type="xsd:integer" use="optional" />
        <xsd:attribute name="disabled" type="xsd:string" use="optional" />
        <xsd:attribute name="deleted" type="xsd:string" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="MailGetNewArrivingEmailRequestType">
      </xsd:complexType>
      <xsd:complexType name="MailGetNewArrivingEmailResponseType">
        <xsd:sequence>
          <xsd:element name="account" type="typens:NewArrivingEmailType"
                       minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="MailAddMailServersRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="server" type="mail:MailServerInfoType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailAddMailServersResponseType">
        <xsd:sequence>
          <xsd:element name="server" type="mail:MailServerInfoType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="MailModifyMailServersRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="server" type="mail:MailServerInfoType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MailModifyMailServersResponseType">
        <xsd:sequence>
          <xsd:element name="server" type="mail:MailServerInfoType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="MailRemoveMailServersRequestType">
        <xsd:sequence>
          <xsd:element name="request_token" type="xsd:string" minOccurs="0" maxOccurs="1" />
          <xsd:element name="server_id" type="base:IDType"
                       minOccurs="1" maxOccurs="unbounded" />
        </xsd:sequence>
