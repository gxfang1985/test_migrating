{strip}
<div class="list_paging_grn {$custom_class}">
    <span class="action_disable_grn top_page_off js_top_page_off">
      <span title="{cb_msg module='grn.grn' key='GRN_GRN-1555' replace='true'}" class="arrow_first_button_grn"></span>
    </span>
    <span class="top_page_on js_top_page_on" style="display:none;" data-page="top">
      <a href="javascript:void(0);" title="{cb_msg module='grn.grn' key='GRN_GRN-1555' replace='true'}" class="action_grn">
          <span class="arrow_first_button_grn"></span>
      </a>
    </span>
    <span class="action_disable_grn prev_page_off js_prev_page_off">
      <span title="{cb_msg module='grn.grn' key='GRN_GRN-1556' replace='true'}{$number_on_page}{cb_msg module='grn.grn' key='GRN_GRN-1557' replace='true'}" class="arrow_left_button_grn"></span>
    </span>
    <span class="prev_page_on js_prev_page_on" style="display:none;" data-page="prev">
      <a href="javascript:void(0);" title="{cb_msg module='grn.grn' key='GRN_GRN-1556' replace='true'}{$number_on_page}{cb_msg module='grn.grn' key='GRN_GRN-1557' replace='true'}" class="action_grn">
          <span class="arrow_left_button_grn"></span>
      </a>
    </span>
    <span class="action_disable_grn next_page_off js_next_page_off" style="">
      <span title="{cb_msg module='grn.grn' key='GRN_GRN-1558' replace='true'}{$number_on_page}{cb_msg module='grn.grn' key='GRN_GRN-1559' replace='true'}" class="arrow_right_button_grn"></span>
    </span>
    <span class="next_page_on js_next_page_on" style="display:none;" data-page="next">
      <a href="javascript:void(0);" title="{cb_msg module='grn.grn' key='GRN_GRN-1558' replace='true'}{$number_on_page}{cb_msg module='grn.grn' key='GRN_GRN-1559' replace='true'}" class="action_grn">
          <span class="arrow_right_button_grn"></span>
      </a>
    </span>
    <span class="sub_title numberList vAlignMiddle-grn mLeft10">
      <span class="start_num js_start_num">0</span>-<span class="end_num js_end_num">0</span>
    </span>
</div>
{/strip}